      "type": "richtext",
      "required": true
    },
    "date": {
      "type": "date",
      "required": true
    },
    "poster": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "media": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "news"
    }
  }
}
