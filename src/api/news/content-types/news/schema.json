{
  "kind": "collectionType",
  "collectionName": "newses",
  "info": {
    "singularName": "news",
    "pluralName": "newses",
    "displayName": "News"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
